{
  "dependencies": [
    "rats",
    "contenttweaker"
  ],
  "populate": {
    
    "milk_lake": {
      "distribution": "uniform",
      "generator": {
        "type": "lake",
        "block": "contenttweaker:cheese_molten",
        "outline-block": "rats:block_of_cheese",
        "use-material": true,
				"material": [
          "grass",
          "stone",
          "sand",
          "sandstone",
          "dirt",
          "leaves",
          "log",
          "tallgrass"
        ]
      },
      "cluster-count": 3,
      "min-height": 20,
      "max-height": 100,
      "follow-terrain": true,
      "biome": "all",
      "retrogen": false,
      "dimension": {"restriction": "whitelist","value": [-8]}
    },

		"ratlantis_ratglowes": {
			"distribution": "surface",
			"generator": {
				"type": "decoration",
				"block": "rats:ratglove_flower",
				"cluster-size": 3
			},
			"cluster-count": 50,
      "biome": "all",
      "dimension": {"restriction": "whitelist","value": [-8]}
		}
  }
}